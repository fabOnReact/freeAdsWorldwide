<%= render "shared/partials/formheader", title: @title %>
        <div class="content">
            <%= simple_form_for @company do |f| %>
              <%= f.input :name, disabled: true %>
              <%= f.association :companytype, prompt: "Choose a type", class: "form-control", disabled: true %>
              <%= f.input :title, disabled: true %>
              <%= f.input :description, as: :text, disabled: true %>
              <%= f.input :website, ad: :url, disabled: true %>
              <%= link_to "Back", companies_path, class: "btn btn-primary btn-fill pull-left" %>
              <%#= f.button :submit, class: "btn btn-info btn-fill pull-right" %>
              <div class="clearfix"></div>
            <% end %>
        </div>
<%= render partial: "shared/partials/formfooter" %>
<%= render partial: "shared/links/companies_links" %>